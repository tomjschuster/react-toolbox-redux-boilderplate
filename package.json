{
  "name": "react-toolbox-redux-boilerplate",
  "version": "1.0.0",
  "description": "React Toolbox boiler plate with redux and simple router.",
  "author": "Tom Schuster <tomjschuster@gmail.com> (https://github.com/tomjschuster)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjschuster/react-toolbox-redux-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/tomjschuster/react-toolbox-redux-boilerplate/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "rm -rf public/* && NODE_ENV=production webpack --config ./config/webpack.config.js --progress --colors",
    "deploy": "git push heroku master",
    "start": "NODE_ENV=production node server",
    "build:dev": "NODE_ENV=development webpack --config ./config/webpack.dev.config.js --progress --colors",
    "start:dev": "npm run build:dev && NODE_ENV=development nodemon --ignore public-dev --ignore app server",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "./config/eslint.config.js"
    ]
  },
  "engines": {
    "node": "8.0.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "history": "^4.6.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "camelcase": "^4.1.0",
    "css-loader": "^0.28.3",
    "eslint": "^3.19.0",
    "eslint-config-fullstack": "^3.0.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "kebab-case": "^1.0.0",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
